{
    "globals": {
        "define": true,
        "gadgets": true
    },
    "env": {
    	"browser": true
    },
    "rules": {
        "quotes": [0],
        "no-console": [1],
        "no-extra-boolean-cast": [0],
        "no-underscore-dangle": [0],
        "no-alert": [1],
        "semi": [0],
        "curly": [0],
        "eol-last": [0],
        "vars-on-top": [1]
    }
}